select from_unixtime(date_ts),num_tasks,procs_per_node,file_per_proc,segment,block_size,transfer_size,aggregate_size,write_data_rate_max,write_data_rate_mean,write_data_rate_min,write_open_time,write_wrrd_time,write_close_time,read_data_rate_max,read_data_rate_mean,read_data_rate_min,read_open_time,read_wrrd_time,read_close_time,command_line from ior where mpihost like 'trinitite' and user like 'XX' and command_line rlike '-F' and command_line rlike '-a MPIIO'and description rlike 'XX' and date_ts>=XX order by date_ts;
